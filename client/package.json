{
  "name": "backbone-react-project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@material-ui/core": "4.5.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/styles": "4.5.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "clsx": "1.0.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-serializer-html": "^7.0.0",
    "prop-types": "15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "7.1.1",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.11.0",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-observable": "~1.2.0",
    "reselect": "4.0.0",
    "rxjs": "6.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint .",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/test-utils/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.13.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2"
  }
}
